@model IEnumerable<IdentityV2.Presentation.Models.ProjectViewModel>

@{
    ViewBag.Title = "IndexForLeader";
    Layout = "~/Views/Shared/_MyTemplatepage.cshtml";
}



<p>
    @Html.ActionLink("Convert View To PDF", "PrintViewToPdf")
</p>
<h2>Available projects :</h2>
<div class="form-group form-focus">
    <label class="control-label">Search Project</label>
    <input type="text" class="form-control floating" id="search" />
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProjectId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateBegin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateEnd)
        </th>
        <th>
            Resources
        </th>

        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr class="search">
            <td>
                @Html.DisplayFor(modelItem => item.ProjectId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateBegin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateEnd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalNbrRessources)
            </td>
        </tr>
    }

</table>
